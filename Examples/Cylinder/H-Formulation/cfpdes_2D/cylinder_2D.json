{
    "Name": "Cylinder 2D",
    "ShortName": "cylinder-2D",
    "Models":
    {
        "cfpdes":{
            "equations":"magnetic"
        },
        "magnetic":
        {
            "name": "magnetic_conductor",
            "setup":{
                "unknown":{
                    "basis":"Ned1h0",
                    "name":"H",
                    "symbol":"H"
                },
                "coefficients":{
                    "zeta":"materials_rho:materials_rho",
                    "d":"materials_mu:materials_mu"
                }
            }
        }
    },
    "Parameters":
    {   
        "mu0":"4*pi*1e-7",
        "bmax":0.02,
        "tf":15,
        "rate":"3.0*bmax/tf:tf:bmax",
        "hsVal":"(t>1.e-10)*(bmax - (t-2.0*tf/3.0)*rate + (t<2.0*tf/3.0)*(t-2.0*tf/3.0)*rate + (t<tf/3.0)*(t*rate - bmax))/mu0:t:tf:rate:bmax:mu0",
        "sigma":58e6,
        "jc":3e8,
        "ec":1e-4,
        "n":20,
        "epsSigma":1e-8
    },
    "Meshes":
    {
        "cfpdes":
        {
            "Import":
            {
                "filename":"$cfgdir/circle.geo"
            }
        }
    },
    "Materials":
    {
        "Conductor":
        {
            "markers":["Conductor"],
            "rho":"ec / jc * (abs(magnetic_curl_H_rt)/jc)^(n - 1):ec:jc:n:magnetic_curl_H_rt",
	        "mu":"mu0:mu0"
        },
        "Air":
        {
	        "markers":["Air"],
            "rho":1,
	        "mu":"mu0:mu0"
        }
    },
    "InitialConditions":    
    {        
        "magnetic":
        {
            "H":        
            {            
                "Expression":            
                {                
                    "Conductor":                
                    {                    
                        "expr":"{0,hsVal}:hsVal"                
                    }            
                }        
            } 
        }
           
    },
    "BoundaryConditions":
    {
        "magnetic":
        {
            "Dirichlet":
            {
                "magdir":
                {
                    "markers":["Infty"],
                    "expr":"{0,hsVal}:hsVal" 
                }
            }
        }
    },
    "PostProcess":
    {
        "use-model-name":1,
        "magnetic":
        {
            "Exports":
            {
                "fields":"H",
                "expr":
                {
                    "B":
                    {
                        "expr":"{materials_mu*magnetic_H_0,materials_mu*magnetic_H_1}:materials_mu:magnetic_H_0:magnetic_H_1",
			            "representation":["element"]
                    },
                    "E":
                    {
                        "expr":"materials_rho*magnetic_curl_H:magnetic_curl_H:materials_rho",
			            "representation":["element"]
                    },
                    "J":
                    {
                        "expr":"magnetic_curl_H:magnetic_curl_H",
                        "representation":["element"]
                    },
                    "hs":
                    {
                        "expr":"{0,hsVal}:hsVal"
                    }
                }
            }
        }
    }
}
