{
    "Name": "Mqs Axisymmetric",
    "ShortName": "mqs-axis",
    "Models":
    {
        "cfpdes":{
            "equations":"magnetic"
        },
        "magnetic":{
            "common":{
                "setup":{
                    "unknown":
                    {
                        "basis":"Pch1",
                        "name":"Atheta",
                        "symbol":"Atheta"
                    }
                }
            },
            "models":[{
                "name":"magnetic_Conductor",
                "materials":"Conductor",
                "setup":{
                    "coefficients":{
                        "c":"x/mu:x:mu",
                        "a":"1/mu/x:mu:x",
                        "f":"-sigma*U/2/pi:sigma:U",
                        "d":"sigma*x:sigma:x"
                    }
                }
            },{
                "name":"magnetic_Air",
                "materials":"Air",
                "setup":{
                    "coefficients":{
                        "c":"x/mu:x:mu",
                        "a":"1/mu/x:mu:x"
                    }
                }
            }]
        }
    },
    "Parameters":
    {
        "sigma":58e6,      
	    "mu":"4*pi*1e-7",   
	    "U":"-1",
        "tf":22,
        "dt":0.1
    },
    "Meshes":
    {
        "cfpdes":
        {
            "Import":
            {
                "filename":"$cfgdir/1torus-axis.geo"
            }
        }
    },
    "Materials":
    {
        "Conductor":
        {
	        "markers":"Conductor",
            "test":17
        },
        "Air":
        {
            "markers":["Air","Air_out"],
            "test1":17,
            "test2":17
        }
    },
    "BoundaryConditions":
    {
        "magnetic":
        {
            "Dirichlet":
            {
                "ZAxis":
                {
                    "expr":"0"
                },
                "Infty":
                {
                    "expr":"0"
                }
            }
        }
    },
    "PostProcess":
    {
        "use-model-name":1,
        "cfpdes":
        {
            "Exports":
            {
                "fields":"Atheta",
                "expr":
                {
                    "B":
                    {
                        "expr":"{-magnetic_grad_Atheta_1,magnetic_grad_Atheta_0+magnetic_Atheta/x}:magnetic_grad_Atheta_0:magnetic_grad_Atheta_1:magnetic_Atheta:x",
                        "representation":["element"]
                    },
                    "j_th":
                    {
                        "expr":"-sigma*(U/2/pi/x+magnetic_dAtheta_dt):sigma:U:x:magnetic_dAtheta_dt",
                        "markers":"Conductor"
                    },
                    "U":
                    {
                        "expr":"U:U"
                    }
                }
            },
            "Measures":
            {
                "Points":
                {
                    "P0":
                    {
                        "coord":"{0,0.}",
                        "fields":[],
                        "expressions":
                        {
                            "U":"U:U"
                        }
                    }
                },
                "Statistics":
                {

                    "I":
                    {
                        "type":"integrate",
                        "expr":"-sigma*(U/2/pi+magnetic_dAtheta_dt*x)/x:sigma:U:magnetic_dAtheta_dt:x",
                        "markers":["Conductor"]
                    }
                }
                
            }
        }
    }
}
